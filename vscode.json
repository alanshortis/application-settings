{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.fontSize": 16,
  "editor.fontFamily": "OperatorMono-Light",
  "editor.formatOnSave": true,
  "editor.lineHeight": 25,
  "files.autoSave": "onFocusChange",
  "explorer.openEditors.visible": 0,
  "editor.accessibilitySupport": "off",
  "editor.fontLigatures": false,
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.emptySelectionClipboard": false,
  "editor.cursorBlinking": "phase",
  "editor.renderIndentGuides": false,
  "files.insertFinalNewline": true,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "terminal.external.osxExec": "Hyper.app",
  "html.format.indentInnerHtml": true,
  "html.format.indentHandlebars": true,
  "html.format.endWithNewline": true,
  "typescript.validate.enable": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.activityBar.visible": true,
  "explorer.confirmDelete": false,
  "workbench.colorTheme": "One Dark Pro Vivid",
  "workbench.colorCustomizations": {
    "gitDecoration.ignoredResourceForeground": "#515151",
    "gitDecoration.modifiedResourceForeground": "#d19a66",
    "gitDecoration.deletedResourceForeground": "#e06c75",
    "gitDecoration.untrackedResourceForeground": "#98c379",
    "gitDecoration.conflictingResourceForeground": "#d19a66"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "keyword",
          "constant",
          "storage.modifier",
          "storage.type.js",
          "storage.type.class.js",
          "storage.type.function.js",
          "keyword.operator.new.js"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["keyword.operator", "keyword.other.unit", "constant.numeric"],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  }
}
