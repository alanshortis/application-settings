{
  "spellright.groupDictionaries": false,
  "editor.minimap.showSlider": "always",
  "emmet.triggerExpansionOnTab": true,
  "nasc-touchbar.showReferences": true,
  "editor.cursorBlinking": "phase",
  "workbench.colorCustomizations": {
    "gitDecoration.ignoredResourceForeground": "#515151",
    "gitDecoration.modifiedResourceForeground": "#D99F2B",
    "gitDecoration.deletedResourceForeground": "#CC3D3D",
    "gitDecoration.conflictingResourceForeground": "#D99F2B",
    "gitDecoration.untrackedResourceForeground": "#86DE74"
  },
  "editor.accessibilitySupport": "off",
  "editor.fontFamily": "OperatorMono-Light",
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.colorTheme": "One Dark Pro",
  "explorer.openEditors.visible": 0,
  "explorer.confirmDragAndDrop": false,
  "breadcrumbs.enabled": true,
  "spellright.documentTypes": ["markdown", "latex", "plaintext", "mdx"],
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.minimap.renderCharacters": false,
  "explorer.confirmDelete": false,
  "nasc-touchbar.addCursorBelow": false,
  "terminal.external.osxExec": "Hyper.app",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.fontSize": 14,
  "oneDarkPro.editorTheme": "One Dark Pro",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "keyword",
          "constant",
          "storage.modifier",
          "storage.type.js",
          "storage.type.class.js",
          "storage.type.function.js",
          "keyword.operator.new.js"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["keyword.operator", "keyword.other.unit", "constant.numeric"],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "nasc-touchbar.togglePanel": false,
  "emmet.includeLanguages": {
    "markdown": "html"
  },
  "workbench.tree.indent": 15,
  "emmet.excludeLanguages": [],
  "nasc-touchbar.showCommands": false,
  "spellright.language": ["en-GB"],
  "editor.fontLigatures": false,
  "nasc-touchbar.jumpToBracket": true,
  "editor.minimap.enabled": false,
  "editor.formatOnSave": true,
  "oneDarkPro.italic": false,
  "workbench.activityBar.visible": true,
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontSize": 16,
  "editor.emptySelectionClipboard": false,
  "editor.lineHeight": 25,
  "security.workspace.trust.untrustedFiles": "open",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "html.format.endWithNewline": true,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "nasc-touchbar.peekDefinition": true,
  "typescript.validate.enable": true,
  "files.autoSave": "off",
  "prettier.requireConfig": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.insertFinalNewline": true,
  "editor.insertSpaces": true,
  "workbench.startupEditor": "newUntitledFile",
  "html.format.indentInnerHtml": true,
  "spellright.parserByClass": {
    "mdx": {
      "parser": "markdown"
    }
  },
  "html.format.indentHandlebars": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "editor.unicodeHighlight.allowedCharacters": {
    "’": true,
    "‘": true
  },
  "window.commandCenter": false,
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": true
  },
  "editor.stickyScroll.enabled": true
}
