{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script"
  },
  "rules": {
    "spaced-comment": 2,
    "eqeqeq": 2,
    "no-alert": 2,
    "semi": 2,
    "camelcase": 2,
    "no-unused-vars": 1,
    "no-delete-var": 2,
    "brace-style": [2, "1tbs", { "allowSingleLine": false }],
    "indent": [2, 2, {"VariableDeclarator": {"var": 2, "let": 2, "const": 3}}],
  }
}
